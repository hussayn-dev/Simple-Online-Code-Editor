<!DOCTYPE html>
<html lang="en">
    <head>
        <title>User Dashboard</title>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
        <meta name="description" content="ComplyNG Dashboard Users" />
        <meta name="author" content="Potenza Global Solutions" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <!-- fontawesome Icon -->
        <script src="https://kit.fontawesome.com/997b229808.js" crossorigin="anonymous"></script>
        <!-- bootstrap-icons -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
        <!-- app favicon -->
        <link rel="shortcut icon" href="assets/img/logo (1).png">
        <!-- google fonts -->
        <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">
        <!-- plugin stylesheets -->
        <link rel="stylesheet" type="text/css" href="assets/css/vendors.css" />
        <!-- app style -->
        <link rel="stylesheet" type="text/css" href="assets/css/styles.css" />
        <!-- chart -->
    </head>
    <body>
        <!-- begin app -->
        <div class="app">
            <!-- begin app-wrap -->
                <!-- begin app-header -->
                <header class="app-header top-bar">
                    <!-- begin navbar -->
                    <nav class="navbar navbar-expand-md">

                        <!-- begin navbar-header -->
                        <div class="navbar-header d-flex align-items-center">
                            <a href="javascript:void:(0)" class="mobile-toggle"><i class="ti ti-align-right"></i></a>
                            
                        </div>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <i class="ti ti-align-left"></i>
                        </button>
                        <!-- end navbar-header -->
                        <!-- begin navigation -->
                        <div class="collapse navbar-collapse" id="navbarSupportedContent">
                            <div class="navigation d-flex">
                                <ul class="navbar-nav nav-left">
                                    <li class="nav-item">
                                        <a href="javascript:void(0)" class="nav-link sidebar-toggle">
                                            <i class="ti ti-align-right"></i>
                                        </a>
                                    </li>
                                </ul>
                             
                                <ul class="navbar-nav nav-right ml-auto">
                                    <li class="nav-item dropdown">
                                        <div class="dropdown-menu extended animated fadeIn" aria-labelledby="navbarDropdown">
                                            
                                        </div>
                                    </li>
                             
    
                                 
                        <!-- end navigation -->
                    </nav>
                    <!-- end navbar -->
                </header>
                <!-- end app-header -->
                <!-- begin app-container -->
                <div class="app-container">
                    <!-- begin app-nabar -->
                    <aside class="app-navbar">
                        <!-- begin sidebar-nav -->
                        <div class="sidebar-nav scrollbar scroll_light" >
                            <ul class="metismenu " id="sidebarNav" >
                                <li>
                                    <a href="/dashboard" aria-expanded="false">
                                        <img src="assets/img/dash.png" draggable="false" alt="">
                                        <span class="nav-title">
                                            Dashboard
                                        </span>
                                    </a> 
                                </li>
                                <li>
                                    <a href="/create" aria-expanded="false">
                                        <img src="assets/img/document.png" draggable="false" alt="">
                                        <span class="nav-title">
                                            Create New
                                        </span>
                                    </a> 
                                </li>
                              
                                <li>
                                    <a class="has-arrow" href="javascript:void(0)"      aria-expanded="false">
                                        <img src="assets/img/setting.png" draggable="false" alt="">
                                        <span class="nav-title">
                                            Settings
                                        </span>
                                    </a>
                                    <ul aria-expanded="false">
                                        <li>
                                            <a href="#">
                                                Edit profile
                                            </a> 
                                        </li>
                                        <li>
                                            <a href="#">
                                                Log out
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <!-- end sidebar-nav -->
                    </aside>
                    <!-- end app-navbar -->
                    <!-- begin app-main -->
                    <div class="app-main" id="main">
                        <!-- begin container-fluid -->
                        <div class="container-fluid">
                            <!-- begin row -->
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="line-div">
                                        <h4 class="hey">Welcome back, <%= username %>
                                            <span>
                                                <%=  time %>
                                            </span>
                                        </h4>
                                        
                                    </div>
                                </div>
                            </div>
                   
                        <!-- Advertising Ends -->
                            <!-- Advertising Ends -->

                            <!-- Services -->
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="tables">
                                        <div class="card-header">
                                            <div class="card-heading mainHeader">
                                                <h4 class="card-title">My Files</h4>
                                                <form class="searchHeader">
                                                    <input type="search" placeholder="Search files">
                                                    <button type="submit"><i class="bi bi-search"></i></button>
                                                </form>
                                            </div>
                                        </div>
                                        <div class="tabel-div">

                                                  <%  if(status) {  %>
                                                    
                                                          <%  if (!data.length) {  %>
                                                            <div class="imagediv">
                                                                <img  src="assets/images/filenotfound.png" alt="">
                                                                <h3>You have no files yet</h3>
                                                            </div>
                                                         

                                                            <%  } else { %>
                                                                <table class="table">
                                                                    <thead>
                                                                      <tr>
                                                                        <th scope="col"></th>
                                                                        <th scope="col">Name</th>
                                                                        <th scope="col">Time</th>
                                                                        <th scope="col">Actions</th>
                                                                      </tr>
                                                                    </thead>
                                                                    <tbody>
                                                     <%   data.forEach(file => { %>
                                                            <tr>
                                                                <th scope="row">-</th>
                                                                <td><%= file.name %>.<%= file.extensionName %></td>
                                                                <td><%= file.date %></td>
                                                                <td><button>View</button> <button>Download</button></td>
                                                              </tr>
                                                      <%  }) %>
                                                           <%  } %>
                                                  <%  } else  { %>
                                                    <div class="imagediv">
                                                        <img  src="assets/images/filenotfound.png" alt="">
                                                        <h3><%= error.message %></h3>
                                                    </div>
                                                     
                                                <%  } %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                              
                <!-- end app-container -->
                <!-- begin footer -->
                <footer class="footer">
                    <div class="row">
                        <div class="col-12 col-sm-12 text-center">
                           <div class="text-center">
                                Copyright © Abdulsamad
                           </div>
                        </div>
                    </div>
                </footer>
                <!-- end footer -->
            </div>
            <!-- end app-wrap -->
        </div>
        <!-- end app -->

        <!-- plugins -->
        <script src="assets/js/vendors.js"></script>

        <!-- custom app -->
        <script src="assets/js/app.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
    </body>
</html>